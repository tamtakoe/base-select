{
  "name": "base-select",
  "version": "0.0.1",
  "description": "Extended select component for web",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --progress --profile --watch --open",
    "start:noShadowDom": "webpack-dev-server --config webpack.config.js --progress --profile --watch --open --noShadowDom",
    "build": "rimraf dist/* && webpack --config webpack.build.config.js --mode production",
    "build:dev": "rimraf dist/* && webpack --mode production",
    "build:old": "rimraf dist/* && tsc -p tsconfig.dist.json"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "bootstrap": "^4.3.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "foundation-sites": "^6.5.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "materialize-css": "^1.0.0",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "main": "./dist/select.js",
  "types": "./dist/select.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamtakoe/base-select"
  },
  "engines": {
    "node": ">= 12.0.0 < 14.0.0",
    "npm": ">= 5"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3"
  }
}
